<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Warehouse_handle">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="16dp"
        android:paddingRight="16dp" >

        <Button
            android:id="@+id/add_prod"
            android:layout_width="535dp"
            android:layout_height="63dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="111dp"
            android:text="Add product" />

        <Button
            android:id="@+id/list_all_prod"
            android:layout_width="537dp"
            android:layout_height="71dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="27dp"
            android:text="List products" />

        <ListView
            android:id="@+id/some_list"
            android:layout_width="wrap_content"
            android:layout_height="250dp"
            android:layout_marginTop="260dp"
            tools:layout_editor_absoluteX="-14dp"
            tools:layout_editor_absoluteY="361dp">

        </ListView>

        <EditText
            android:id="@+id/man_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="260dp"
            android:gravity="center"
            android:inputType="text"
            android:hint="Manufacutre name" />

        <EditText
            android:id="@+id/model_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="320dp"
            android:gravity="center"
            android:inputType="text"
            android:hint="Model name" />

        <EditText
            android:id="@+id/quantity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="380dp"
            android:gravity="center"
            android:hint="Quantity"
            android:inputType="text" />

        <EditText
            android:id="@+id/price"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginStart="12dp"
            android:layout_marginTop="440dp"
            android:hint="Price"
            android:inputType="text" />

        <Button
            android:id="@+id/submit"
            android:layout_width="537dp"
            android:layout_height="71dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="500dp"
            android:text="Submit" />

        <Button
            android:id="@+id/return_to_main"
            android:layout_width="200dp"
            android:layout_height="63dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"

            android:layout_marginTop="175dp"
            android:text="Return to main menu" />

        <Button
            android:id="@+id/net_on_off"
            android:layout_width="200dp"
            android:layout_height="63dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="175dp"
            android:layout_marginEnd="200dp"
            android:text="UNDEFINED" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>